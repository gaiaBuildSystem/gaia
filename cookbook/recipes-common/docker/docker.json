{
    "name": "docker",
    "type": "package",
    "priority": 0,
    "support": [
        "linux/amd64",
        "linux/arm64"
    ],
    "env": {
        "DOCKER_ALTERNATIVES_ADMINDIR": "/var/lib/dpkg/alternatives"
    },
    "targetDeps": [
        "iptables",
        "netbase",
        "libnftables1",
        "libxtables12"
    ],
    "beforeDeployRecipes": [
        "addSource.ts"
    ],
    "deployRecipes": [
        "deploy.ts"
    ],
    "afterDeployRecipes": [
        "setupIptableLegacy.ts"
    ]
}
